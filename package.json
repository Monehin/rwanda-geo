{
  "name": "rwanda-geo",
  "version": "1.0.6",
  "description": "ðŸ‡·ðŸ‡¼ Complete, typed, and lightweight dataset of Rwanda's administrative divisions - Provinces, Districts, Sectors, Cells, Villages with official codes and coordinates",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./data/*": {
      "import": "./dist/data/*.mjs",
      "require": "./dist/data/*.js"
    },
    "./boundaries/*": {
      "import": "./dist/boundaries/*.mjs",
      "require": "./dist/boundaries/*.js"
    }
  },
  "files": [
    "dist/index.mjs",
    "dist/index.d.ts",
    "src/data/*.json.gz",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "npm run gzip-data",
    "build": "tsup",
    "prepostbuild": "rm -rf dist/data",
    "postbuild": "cpx 'src/data/*.json.gz' dist/data/",
    "build:all": "npm run clean && npm run build",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "gzip-data": "node scripts/gzip-data.js",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major"
  },
  "keywords": [
