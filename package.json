{
  "name": "rwanda-geo",
  "version": "1.0.3",
  "description": "A reliable, typed, and lightweight dataset of Rwanda's administrative divisions with official codes and center coordinates",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./data/*": {
      "import": "./dist/data/*.mjs",
      "require": "./dist/data/*.js"
    },
    "./boundaries/*": {
      "import": "./dist/boundaries/*.mjs",
      "require": "./dist/boundaries/*.js"
    }
  },
  "files": [
    "dist/index.mjs",
    "dist/index.d.ts",
    "src/data/*.json.gz",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "npm run gzip-data",
    "build": "tsup",
    "prepostbuild": "rm -rf dist/data",
    "postbuild": "cpx 'src/data/*.json.gz' dist/data/",
    "build:all": "npm run clean && npm run build",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "gzip-data": "node scripts/gzip-data.js"
  },
  "keywords": [
    "rwanda",
    "geography",
    "administrative",
    "provinces",
    "districts",
    "sectors",
    "cells",
    "villages",
    "geojson",
    "geospatial",
    "civic-tech",
    "government",
    "africa"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monehin/rwanda-geo.git"
  },
  "bugs": {
    "url": "https://github.com/monehin/rwanda-geo/issues"
  },
  "homepage": "https://github.com/monehin/rwanda-geo#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "cpx": "^1.5.0",
    "eslint": "^8.57.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^7.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
